    "react-spring": "^6.1.8",
    "react-transition-group": "^2.5.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
